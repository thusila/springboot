pipeline {
    agent any
    tools {
        maven '3.2.1'
    }
    stages {
        stage('Version') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('Scan') {
            steps {
                withSonarQubeEnv(installationName: 'sonar-6') { 
                sh 'mvn clean install sonar:sonar
                }
            }
        }
        
    }
}
