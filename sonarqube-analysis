pipeline {
    agent any
    tools {
        maven '3.8.4'
    }
    stages {
        stage('Version') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('Scan') {
            steps {
                withSonarQubeEnv(installationName: 'sonar-6') { 
                sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar'
                }
            }
        }
        
    }
}
